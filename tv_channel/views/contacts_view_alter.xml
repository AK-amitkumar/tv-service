<openerp>
<data>
    <record id="view_partner_form_inh" model="ir.ui.view">
        <field name="name">res.partner.form.inherit</field>
        <field name="model">res.partner</field>
        <field eval="258" name="priority"/>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet//notebook" position="inside">
                <page string="Channels">
                    <field name="channel_ids" nolabel="1" widget="many2many_list"/>
                    <div>
                        <br/>
                        <button name="load_channels" string="Import Channels" type="object"/>
                        <br>Import channels form CSV file. File must contain only one column with channel names. Avoid ending spaces,quotes, tabs, commas and so on.</br>
                    </div>
                </page>
            </xpath>
            <field name="category_id" position="before">
                <field name="customer_type"/>
            </field>
            <field name="phone" position="before">
                <field name="service_type"/>
            </field>
        </field>
    </record>
</data>
</openerp>
