<data noupdate="0">
    <record model="ir.rule" id="tv_visibility_tv_user">
        <field name="name">Own records tv_user</field>
        <field name="model_id" ref="tv_channel.model_tv_channel"/>
        <field name="domain_force">
            [
                ('create_uid', '=', user.id),
            ]
        </field>
        <field name="groups" eval="[(4,ref('tv_user'))]"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="0"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="0"/>
    </record>
    <record model="ir.rule" id="tv_visibility_tv_manager">
